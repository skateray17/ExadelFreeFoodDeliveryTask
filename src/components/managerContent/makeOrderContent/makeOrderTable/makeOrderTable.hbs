<div id="make-order__printableArea-table">
    <div class="make-order__table-header">
        <span class="make-order__dish-header-title">{{i18n 'managerPage.makeAnOrder.dishTitle'}}</span><span
            class="make-order__dish-amount">{{i18n 'managerPage.makeAnOrder.amount'}}</span>
    </div>
    <div class="make-order__table">
        {{#each items}}

            <div class="make-order_order-item">
                <span class="make-order__dish-title">{{dishTitle}}</span>
                <span class="make-order__amount">{{amount}}</span>
            </div>

        {{/each}}
    </div>
</div>